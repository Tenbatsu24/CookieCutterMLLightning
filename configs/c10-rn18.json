{
  "batch_size": 128,
  "dataset": {
    "name": "c10"
  },
  "loss": {
    "params": {
      "label_smoothing": 1e-05,
      "reduction": "mean",
      "weight": null
    },
    "type": "CrossEntropyLoss"
  },
  "metrics": {
    "acc": {
      "params": {
        "average": "macro",
        "num_classes": 10,
        "task": "multiclass"
      },
      "type": "Accuracy"
    }
  },
  "model": {
    "params": {
      "in_channels": 3,
      "num_classes": 10
    },
    "type": "rn18"
  },
  "opt": {
    "params": {
      "lr": 0.1,
      "momentum": 0.9,
      "nesterov": true,
      "weight_decay": 2e-05
    },
    "type": "SGD"
  },
  "pl": {
    "checkpoint": {
      "mode": "max",
      "monitor": "val/acc"
    },
    "trainer": {
      "accelerator": "gpu",
      "accumulate_grad_batches": 1,
      "benchmark": true,
      "deterministic": true,
      "devices": 1,
      "gradient_clip_algorithm": "norm",
      "gradient_clip_val": 0.5,
      "limit_val_batches": 1.0,
      "max_epochs": 10,
      "num_sanity_val_steps": 0,
      "precision": "32",
      "strategy": "auto",
      "val_check_interval": 1.0
    }
  },
  "run_id": null,
  "run_name": "c10-rn18",
  "scheduler": [
    [
      "lr",
      "CosSched(0.1, 1e-6)"
    ],
    [
      "weight_decay",
      "CosSched(2e-5, 1e-4)"
    ]
  ]
}
